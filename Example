import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

class MessageListResponseTest {

    @Test
    void testGetSetCustId() {
        MessageListResponse response = new MessageListResponse();
        assertNull(response.getCustId());

        // Set customer ID and test
        String custId = "123";
        response.setCustId(custId);
        assertEquals(custId, response.getCustId());
    }

    @Test
    void testGetSetECPDId() {
        MessageListResponse response = new MessageListResponse();
        assertNull(response.getECPDId());

        // Set ECPD ID and test
        String eCPDId = "456";
        response.setECPDId(eCPDId);
        assertEquals(eCPDId, response.getECPDId());
    }

    @Test
    void testGetSetCustType() {
        MessageListResponse response = new MessageListResponse();
        assertNull(response.getCustType());

        // Set customer type and test
        String custType = "Individual";
        response.setCustType(custType);
        assertEquals(custType, response.getCustType());
    }

    @Test
    void testGetSetErrorCode() {
        MessageListResponse response = new MessageListResponse();
        assertEquals("0000", response.getErrorCode());

        // Set error code and test
        String errorCode = "1001";
        response.setErrorCode(errorCode);
        assertEquals(errorCode, response.getErrorCode());
    }

    @Test
    void testGetSetErrorMsgTxt() {
        MessageListResponse response = new MessageListResponse();
        assertEquals("SUCCESS", response.getErrorMsgTxt());

        // Set error message text and test
        String errorMsgTxt = "Error occurred";
        response.setErrorMsgTxt(errorMsgTxt);
        assertEquals(errorMsgTxt, response.getErrorMsgTxt());
    }

    @Test
    void testGetSetMessage() {
        MessageListResponse response = new MessageListResponse();
        assertNull(response.getMessage());

        // Create a sample list of messages
        List<CCESMessageInformation> messages = new ArrayList<>();
        CCESMessageInformation message1 = new CCESMessageInformation(/* provide necessary arguments */);
        CCESMessageInformation message2 = new CCESMessageInformation(/* provide necessary arguments */);
        messages.add(message1);
        messages.add(message2);

        // Set messages and test
        response.setMessage(messages);
        assertEquals(messages, response.getMessage());
    }
}
