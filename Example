import org.junit.jupiter.api.Test;
import static org.junit.jupiter.api.Assertions.*;

class SearchDocumentsResponseTest {

    @Test
    void testGetSetSearchCriteria() {
        SearchDocumentsResponse response = new SearchDocumentsResponse();
        assertNull(response.getSearchCriteria());

        // Create a sample search criteria
        SearchCriteria searchCriteria = new SearchCriteria(/* provide necessary arguments */);

        // Set search criteria and test
        response.setSearchCriteria(searchCriteria);
        assertEquals(searchCriteria, response.getSearchCriteria());
    }

    @Test
    void testGetSetSearchResults() {
        SearchDocumentsResponse response = new SearchDocumentsResponse();
        assertNull(response.getSearchResults());

        // Create a sample search results
        SearchResults searchResults = new SearchResults(/* provide necessary arguments */);

        // Set search results and test
        response.setSearchResults(searchResults);
        assertEquals(searchResults, response.getSearchResults());
    }

    @Test
    void testGetSetErrorCode() {
        SearchDocumentsResponse response = new SearchDocumentsResponse();
        assertNull(response.getErrorCode());

        // Set error code and test
        String errorCode = "500";
        response.setErrorCode(errorCode);
        assertEquals(errorCode, response.getErrorCode());
    }

    @Test
    void testGetSetErrorMsgTxt() {
        SearchDocumentsResponse response = new SearchDocumentsResponse();
        assertNull(response.getErrorMsgTxt());

        // Set error message text and test
        String errorMsgTxt = "Internal Server Error";
        response.setErrorMsgTxt(errorMsgTxt);
        assertEquals(errorMsgTxt, response.getErrorMsgTxt());
    }
}
