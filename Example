import static org.junit.jupiter.api.Assertions.*;
import org.junit.jupiter.api.Test;
import org.springframework.context.annotation.AnnotationConfigApplicationContext;

class SwaggerConfigsTest {

    @Test
    void testSwaggerDocketBeanCreation() {
        try (AnnotationConfigApplicationContext context = new AnnotationConfigApplicationContext(SwaggerConfigs.class)) {
            // Check if the Docket bean is created
            assertNotNull(context.getBean(Docket.class));
        }
    }

    @Test
    void testCorsFilterBeanCreation() {
        try (AnnotationConfigApplicationContext context = new AnnotationConfigApplicationContext(SwaggerConfigs.class)) {
            // Check if the CorsFilter bean is created
            assertNotNull(context.getBean(CorsFilter.class));
        }
    }

    @Test
    void testApiInfoCreation() {
        SwaggerConfigs swaggerConfigs = new SwaggerConfigs();
        assertNotNull(swaggerConfigs.metaData());
    }
}
